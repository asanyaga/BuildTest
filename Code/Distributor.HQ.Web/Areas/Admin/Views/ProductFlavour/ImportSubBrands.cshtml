@model Distributr.HQ.Lib.ViewModels.Admin.Product.ProductFlavoursViewModel

@{
    ViewBag.Title = "ImportSubBrands";
}


<h2 id="breadcrumb-holder">Products &amp; Pricing &gt; <span class="bc-example">@Html.ActionLink("View Sub Brands", "/ListProductFlavours")</span> &gt; Import Sub Brand</h2>


<div id="main-content-housing">



<p  style="color:Red;">
    @ViewBag.msg
</p>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("ImportSubBrands", "ProductFlavour", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="editor-field">
    <input type="file" name="file" />
    </div>
    <input type="submit" value="OK" class="button-style-one"/>
}
</div>


