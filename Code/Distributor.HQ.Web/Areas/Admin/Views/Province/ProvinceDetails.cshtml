@model Distributr.HQ.Lib.ViewModels.Admin.CostCenter.ProvinceViewModel

@{
    ViewBag.Title = "ProvinceDetails";
}

<h2>ProvinceDetails</h2>

<fieldset>
  @*  <legend>ProvinceViewModel</legend>*@

    <div class="display-label">Name</div>
    <div class="display-field">@Model.Name</div>

    <div class="display-label">Description</div>
    <div class="display-field">@Model.Description</div>

    <div class="display-label">CountryId</div>
    <div class="display-field">@Model.Country</div>

    <div class="display-label">IsActive</div>
    <div class="display-field">@Model.IsActive</div>
</fieldset>
<p>
    @Html.ActionLink("Edit", "EditProvince", new { id=Model.Id }) |
    @Html.ActionLink("Back to List", "ListProvinces")
</p>

