@model Distributr.HQ.Lib.ViewModels.Agrimanagr.SettingsViewModel.AgrimanagrReportSettingViewModel

@{
    ViewBag.Title = "Report Setting";
}
<h2 id="breadcrumb-holder">
Global Settings &gt; Report Settings
</h2>
<div id="main-content-housing">
    <h2 class="page-header">Reports Setting</h2>
    @if (ViewBag.msg != null)
    {
        <div class="notice-notification">
            @ViewBag.msg
        </div>
    }
    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()
        @Html.ValidationSummary(true)

        <fieldset>
            <legend>Reports  Setting </legend>

            <div class="editor-label">
                @Html.LabelFor(model => model.Server)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.Server)
                @Html.ValidationMessageFor(model => model.Server)
            </div>

            <div class="editor-label">
                @Html.LabelFor(model => model.ReportUsername)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.ReportUsername)
                @Html.ValidationMessageFor(model => model.ReportUsername)
            </div>
             <div class="editor-label">
                @Html.LabelFor(model => model.ReportFolder)
            </div>
            <div class="editor-field">
                @Html.EditorFor(model => model.ReportFolder)
                @Html.ValidationMessageFor(model => model.ReportFolder)
            </div>
            <div class="editor-label">
                @Html.LabelFor(model => model.ReportPassword)
            </div>
            <div class="editor-field">
                @Html.PasswordFor(model => model.ReportPassword)
                @Html.ValidationMessageFor(model => model.ReportPassword)
            </div>

           @* <p>
                <input type="submit" value="Save" />
            </p>*@
        </fieldset>
        <fieldset class="actions">
<legend>Actions</legend>
<input name="Submit" type="submit" value="Save" class="button-style-one"/>
</fieldset>
    }
</div>


