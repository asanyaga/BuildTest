@model Distributr.HQ.Lib.ViewModels.Admin.Product.AdminProductPackagingViewModel

@{
    ViewBag.Title = "ImportProductPackaging";
}

<h2 id="breadcrumb-holder">Products &amp; Pricing &gt; <span class="bc-example">@Html.ActionLink("View Packaging", "/ListProductPackaging")</span> &gt; Import Packaging</h2>

<div id="main-content-housing">

<br />
<p  style="color:Red;">
    @ViewBag.msg
</p>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("ImportProductPackaging", "ProductPackaging", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
      <input type="file" name="file" />
    <input type="submit" value="OK" class="button-style-one" />
    
}
</div>


