@model Distributr.HQ.Lib.ViewModels.Admin.Product.ProductPricingTierViewModel

@{
    ViewBag.Title = "ImportPricingTier";
}
	<h2 id="breadcrumb-holder">Products &amp; Pricing &gt; <span class="bc-example">@Html.ActionLink("View Pricing Tiers", "/ListProductPricingTiers")</span> &gt; Import Pricing Tier</h2>

<div id="main-content-housing">

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
<p  style="color:Red;">
    @ViewBag.msg
</p>
@using (Html.BeginForm("ImportPricingTiers", "ProductPricingTier", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
    <input type="file" name="file" />
    <input type="submit" value="OK"  class="button-style-one"/>
}

</div>
