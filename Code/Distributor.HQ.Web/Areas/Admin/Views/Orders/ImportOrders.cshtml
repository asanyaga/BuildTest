@model Distributr.HQ.Lib.ViewModels.Admin.Orders.OrderViewModel

@{
    ViewBag.Title = "ImportOrders";
}
<h2 id="breadcrumb-holder">ORDERS &gt;<span class="bc-example"> @Html.ActionLink("List All Orders", "/ListAllOrders") </span>&gt;Import Orders</h2>
 <div id="main-content-housing">
 <h2 class="page-header">Import Orders</h2>

<div style="display:none;">
   
</div>



@if (ViewBag.msg != null)
{ 
    <div class="notice-notification">
    @ViewBag.msg
</div>
}
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("ImportOrders", "Orders", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
     @Html.ValidationSummary(true)
    <div class="editor-field">
    <input type="file" name="file" />
    </div>
    <input type="submit" value="OK"  class="button-style-one"/>
}
</div>