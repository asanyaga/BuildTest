@model Distributr.HQ.Lib.ViewModels.Admin.CostCenter.OutletCategoryViewModel

@{
    ViewBag.Title = "ImportOutletCategory";
}


<div><p class="add-items">
    @Html.ActionLink("View Outlet Category", "/ListOutletCategories")</p>
</div>
<div style="display:none;">
   
</div>
<h2 class="page-intro-header">Import Outlet Category</h2>


<p  style="color:Red;">
    @ViewBag.msg
</p>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("ImportOutletCategory", "OutletCategory", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="editor-field">
    <input type="file" name="file" />
    </div>
    <input type="submit" value="OK" class="add-items-button"/>
}

