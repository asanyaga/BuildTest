@model Distributr.HQ.Lib.ViewModels.Admin.Product.AdminProductTypeViewModel

@{
    ViewBag.Title = "ImportProductType";
}
	<h2 id="breadcrumb-holder">Products &amp; Pricing &gt; <span class="bc-example">@Html.ActionLink("View Product Type", "/ListProductTypes")</span> &gt; Add Product Type</h2>

<div id="main-content-housing">
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

@using (Html.BeginForm("ImportProductType", "ProductType", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.ValidationSummary(true)
<input type="file" name="file" />
    <input type="submit" value="OK"  class="button-style-one"/>
}

</div>
