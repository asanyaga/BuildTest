        </Field>
        <Field Name="TotalSales">
          <DataField>TotalSales</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalLosses">
          <DataField>TotalLosses</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalCloseOfDay">
          <DataField>TotalCloseOfDay</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalPurchaseOrders">
          <DataField>TotalPurchaseOrders</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalInventoryTransfer">
          <DataField>TotalInventoryTransfer</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="OpeningBalance">
          <DataField>OpeningBalance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="StockTake">
          <DataField>StockTake</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="TotalOutflow">
          <Value>=Fields!TotalSales.Value + Fields!TotalInventoryTransfer.Value + Fields!TotalSamplesIssued.Value + Fields!TotalLosses.Value</Value>
        </Field>
        <Field Name="TotalInflow">
          <Value>=Fields!OpeningBalance.Value + Fields!TotalPurchaseOrders.Value  + Fields!TotalCloseOfDay.Value + Fields!StockAdjustment.Value + Fields!TotalStockReturns.Value</Value>
        </Field>
        <Field Name="StockTakeDate">
          <DataField>StockTakeDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ClosingStock">
          <Value>=Fields!TotalInflow.Value - Fields!TotalOutflow.Value</Value>
        </Field>
        <Field Name="StockVariance">
          <Value>=Fields!ClosingStock.Value - Fields!StockTake.Value</Value>
        </Field>
      </Fields>
