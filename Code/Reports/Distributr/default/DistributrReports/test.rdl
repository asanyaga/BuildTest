  <DataSets>
    <DataSet Name="Values">
      <Query>
        <DataSourceName>Master</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select dist.Id DistributorId,
       dist.Name DistributorName,
	   sm.Id SalesmanId,
	   sm.Name SalesmanName,
	   CONVERT(NVARCHAR(26),sale.DocumentDateIssued,23) SaleDate,
	   SUM(sli.Quantity * (sli.Value + sli.Vat)) TotalSales
from  tblDocument sale
inner join tblLineItems sli on sale.Id = sli.DocumentID
inner join tblCostCentre dist on sale.DocumentIssuerCostCentreId=dist.Id or sale.DocumentRecipientCostCentre=dist.Id
inner join tblCostCentre sm on sale.DocumentIssuerCostCentreId=sm.Id or sale.DocumentRecipientCostCentre=sm.Id
where dist.CostCentreType = 2 
  and sm.CostCentreType = 4
  and CONVERT(NVARCHAR(26),sale.DocumentDateIssued,23) Between @StartDate and @EndDate

group by dist.id,dist.Name,sm.Id,sm.Name,CONVERT(NVARCHAR(26),sale.DocumentDateIssued,23) 
order by CONVERT(NVARCHAR(26),sale.DocumentDateIssued,23) desc</CommandText>
      </Query>
