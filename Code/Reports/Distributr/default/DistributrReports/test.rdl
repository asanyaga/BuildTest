  <Code> Public Shared Function Total(ByVal Tot As Decimal) As Integer
	Dim round As Decimal
	Dim Val As Integer
	round=Tot Mod 1
Select Case round
		Case Is &gt;=0.04
			Val=Math.Ceiling(Tot)
		Case Else
			Val=Math.Floor(Tot)
	End Select
	Return Val
End Function




