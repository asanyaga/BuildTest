<Application x:Class="Integration.QuickBooks.WPF.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:locator="clr-namespace:Integration.QuickBooks.Lib.IOC;assembly=Integration.QuickBooks.Lib"
             StartupUri="Views/LoginPage.xaml">
    <Application.Resources>
        <ResourceDictionary>
        <locator:QBViewModelLocator x:Key="QBViewModelLocator"/>
        <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Assets/Styles.xaml" />
                <ResourceDictionary Source="Assets/AdminMenuStyle.xaml" />
                <ResourceDictionary Source="Assets/TabStyles.xaml" />
        </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
