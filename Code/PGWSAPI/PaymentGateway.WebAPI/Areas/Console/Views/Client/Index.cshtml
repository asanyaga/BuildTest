@using PaymentGateway.WSApi.Lib.Paging;

@model IPagedList<PaymentGateway.WSApi.Lib.Domain.MasterData.Client>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table>
    <tr>
       
        <th>
            Name
        </th>
        <th>
            Keyword
        </th>
        <th>
            Uri
        </th>
        <th>
            App
        </th>
        <th>
            App Password
        </th>        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
       
        <td>
            @item.Name
        </td>
        <td>
            @item.Code
        </td>
        <td>
            @item.Path
        </td>
        <td>
            @item.ApplicationId
        </td>
        <td>
          @ item.ApplicationPassword
        </td>
         <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
           
           
        </td>
    </tr>
}

</table>
<div class="pager">		
        @Html.Pager(Model.PageSize, Model.PageNumber, Model.TotalItemCount)
</div>  
</div>
